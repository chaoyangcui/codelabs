<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:id="$+id:parent"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:background_element="$media:bg_music_player"
    ohos:alignment="center">

    <com.huawei.harmonyaudiodemo.player.view.PlayerLoading
        ohos:id="$+id:loadint_view"
        ohos:height="match_parent"
        ohos:width="match_parent"/>

    <com.huawei.harmonyaudiodemo.player.view.SimplePlayerController
        ohos:id="$+id:controller_view"
        ohos:height="match_parent"
        ohos:width="match_parent"/>

</DependentLayout>
